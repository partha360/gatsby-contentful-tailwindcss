    "gatsby-link": "1.6.41",
    "gatsby-plugin-i18n": "^0.4.2",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-source-contentful": "^1.3.48",
    "gatsby-transformer-remark": "1.7.40",
    "inquirer": "^5.2.0",
    "intl": "^1.2.5",
    "lodash": "^4.17.10",
    "purgecss": "^0.21.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "slash": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-eslint": "^8.2.3",
    "chalk": "^2.4.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.1",
    "extract-text-webpack-plugin": "next",
    "gatsby-cli": "^1.1.51",
    "gh-pages": "^1.1.0",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.12.1",
    "request": "^2.85.0",
    "tailwindcss": "^0.5.2"
  },
  "keywords": ["gatsby", "contentful", "tailwindcss"],
  "license": "MIT",
  "scripts": {
    "tailwind:build":
      "postcss src/styles/tailwind.index.css -o src/styles/index.css",
    "tailwind:watch":
      "postcss src/styles/tailwind.index.css -o src/styles/index.css -w",
    "build": "npm run tailwind:build && gatsby build",
    "develop": "gatsby develop & npm run tailwind:watch",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node ./bin/setup.js"
  }
}
