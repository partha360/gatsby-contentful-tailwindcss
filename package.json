{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Parthasarathy S",
  "dependencies": {
    "contentful-import": "^7.0.1",
    "eslint-plugin-react": "^7.7.0",
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-source-contentful": "^1.3.47",
    "gatsby-transformer-remark": "next",
    "inquirer": "^5.2.0",
    "lodash": "^4.17.5",
    "purgecss": "^0.21.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.3",
    "chalk": "^2.4.0",
    "eslint": "^4.19.1",
    "extract-text-webpack-plugin": "next",
    "gatsby-cli": "^1.1.50",
    "gh-pages": "^1.1.0",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.12.1",
    "request": "^2.85.0",
    "tailwindcss": "^0.5.2"
  },
  "keywords": ["gatsby", "contentful", "tailwindcss"],
  "license": "MIT",
  "scripts": {
    "tailwind:build":
      "postcss src/layouts/tailwind.index.css -o src/layouts/index.css",
    "tailwind:watch":
      "postcss src/layouts/tailwind.index.css -o src/layouts/index.css -w",
    "build": "npm run tailwind:build && gatsby build",
    "develop": "gatsby develop & npm run tailwind:watch",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node ./bin/setup.js"
  }
}
